<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>

<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<title>用户注册</title>
</head>
<body>
	<form action="/servlet1/user1" method="post" enctype="multipart/form-data">
		用户ID<input type="number" name="id" id="id"><br>
		<br>用户名<input type="text" name="account"><br>
		<br>昵称<input type="text" name="nickName"><br>
		<br>密码<input type="password" name="password"><br>
		<br> 年龄<input type="number" name="age"><br>
		<br> 性别<input type="radio" name="sex" value="男">男 <input
			type="radio" name="sex" value="女">女<br>
		<br> 头像<input type="file" name="img"><br>

		<button type="reset" name=reset>重置</button>
		<button type="submit" name=login>注册</button>
	</form>
</body>
</html>